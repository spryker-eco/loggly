{
  "name": "spryker-eco/loggly",
  "description": "Loggly module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/monolog": "*",
    "spryker/queue": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/queue": "Needed to process messages from the queue",
    "spryker/monolog": "Needed to send messages to loggly via Curl helper"
  },
  "autoload": {
    "psr-4": {
      "SprykerEco\\": "src/SprykerEco/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ]
}
